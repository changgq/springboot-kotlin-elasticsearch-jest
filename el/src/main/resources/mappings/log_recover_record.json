{
  "settings": {
    "index": {
      "number_of_shards": 1,
      "number_of_replicas": 1
    }
  },
  "mappings": {
    "doc": {
      "_source": {
        "enabled": true
      },
      "properties": {
        "id": {
          "type": "keyword"
        },
        "logType": {
          "type": "keyword"
        },
        "recoverDate": {
          "type": "date",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        "recoverTimes": {
          "type": "long"
        },
        "fileName": {
          "type": "keyword"
        },
        "filePath": {
          "type": "keyword"
        },
        "fileSize": {
          "type": "long"
        },
        "recoverStatus": {
          "type": "boolean"
        }
      }
    }
  }
}