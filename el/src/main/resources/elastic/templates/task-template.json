{
  "template": "task-template",
  "index_patterns": [
    "task*"
  ],
  "order": 1,
  "settings": {
    "index.number_of_shards": 1,
    "number_of_replicas": 1
  },
  "mappings": {
    "doc": {
      "_source": {
        "enabled": true
      },
      "dynamic": "strict",
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "@version": {
          "type": "keyword"
        },
        "id": {
          "type": "keyword"
        },
        "taskName": {
          "type": "keyword"
        },
        "taskCode": {
          "type": "keyword"
        },
        "taskCreateDate": {
          "type": "date",
          "format": "yyyy-MM-dd HH:mm:ss"
        },
        "taskExecuteTimes": {
          "type": "long"
        },
        "taskFirstExecute": {
          "type": "long"
        },
        "taskFrequency": {
          "type": "keyword"
        },
        "taskVersion": {
          "type": "long"
        },
        "taskStatus": {
          "type": "boolean"
        }
      }
    }
  }
}